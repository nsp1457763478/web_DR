define(function(q,t,r){q("jquery"),r.exports=function(e){var b=this;b.options={dom:"",callback:function(){}},$.extend(b.options,e);var f,g=!1,h=!0;if(e=0!=Number($(b.options.dom).attr("data-time"))?Number($(b.options.dom).attr("data-time")):$(b.options.dom).attr("data-endDate").replace(/-/g,"/"),0!=Number($(b.options.dom).attr("data-time"))){var d=new Date;d.setTime((new Date).getTime()+1e3*e)}else d=new Date(e);var k=0,l=0,m=0,n=0,p=0;this.ob={time:0,zero:function(a){return 10>a?"0"+a:a},interceptionMsStr:function(a){return a=a.toString(),a=Number(a.substring(a.length-3)),100>a?"0"+a:10>a?"00"+a:a},count:function(){var a=this;h&&(h=!1,f=setInterval(function(){a.count()},1));var c=d.getTime()-(new Date).getTime();0<=c?(k=Math.floor(c/1e3/60/60/24),l=Math.floor(c/1e3/60/60%24),m=Math.floor(c/1e3/60%60),n=Math.floor(c/1e3%60),this.time=p=c,this.view()):($(b.options.dom).find("span.t_ms").text("000"),$(b.options.dom).attr("data-timeRemaining",0),b.options.callback&&b.options.callback(),clearInterval(f))},view:function(){$(b.options.dom).find("span.t_d").text(this.zero(k)),$(b.options.dom).find("span.t_h").text(this.zero(l)),$(b.options.dom).find("span.t_m").text(this.zero(m)),$(b.options.dom).find("span.t_s").text(this.zero(n)),$(b.options.dom).find("span.t_ms").text(this.interceptionMsStr(p))},pause:function(){g||(clearInterval(f),g=!0,$(b.options.dom).attr("data-timeRemaining",this.time))},continueFun:function(){if(g){g=!1;var a=Number($(b.options.dom).attr("data-timeRemaining"));0!=a&&(d=new Date,d.setTime((new Date).getTime()+a),f=setInterval(function(){b.ob.count()},1))}}},this.ob.count()}});