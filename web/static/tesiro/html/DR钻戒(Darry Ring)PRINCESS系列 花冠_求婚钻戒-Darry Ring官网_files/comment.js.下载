define(function(require,exports,module){function getPinlun(res){$(".Details-sec").html(res),AllaskRight();for(var haveimgList=$(".haveimg_slt"),tIndexArry=[],i=0;i<haveimgList.length;i++){var tId="#haveimg_slt"+i;(new Imgscroll).init({id:tId,prev:tId+" .haveimg_preend,"+tId+" .haveimg_pre",next:tId+" .haveimg_next,"+tId+" .haveimg_nextend",scrollNum:1,theMag:14,theway:!1,Alllist:4,thebutton:!0,ntendClass:"haveimg_nextend",ntClass:"haveimg_next",pendClass:"haveimg_preend",peClass:"haveimg_pre"}),tIndexArry[i]=0,$("#haveimg_theimg"+i+" .Bigimg_next").live("click",function(){var tlist=$(this).parents(".haveimg_theimg"),tsmallList=$(this).parents(".haveimg_theimg").prev().find("li");tIndexArry[i]=tIndexArry[i]+1>=tsmallList.length?0:tIndexArry[i]+1,tlist.find(".haveimg_bigimg li").eq(tIndexArry[i]).stop(!0,!0).show().siblings().hide(),tsmallList.eq(tIndexArry[i]).addClass("haveimg_sltall-click").siblings().removeClass("haveimg_sltall-click"),AllaskRight()}),$("#haveimg_theimg"+i+" .Bigimg_pre").live("click",function(){var tlist=$(this).parents(".haveimg_theimg"),tsmallList=$(this).parents(".haveimg_theimg").prev().find("li");tIndexArry[i]=tIndexArry[i]-1,tlist.find(".haveimg_bigimg li").eq(tIndexArry[i]).stop(!0,!0).show().siblings().hide(),tsmallList.eq(tIndexArry[i]).addClass("haveimg_sltall-click").siblings().removeClass("haveimg_sltall-click"),AllaskRight()}),$("#haveimg_slt"+i+" .haveimg_sltall li").each(function(index){$(this).live("click",function(){var tlist=$(this).parents(".haveimg_slt").next(),tsmallList=$(this).parents(".haveimg_slt").find("li");tlist.show(),tIndexArry[i]=index,tlist.find(".haveimg_bigimg li").eq(tIndexArry[i]).stop(!0,!0).show().siblings().hide(),tsmallList.eq(tIndexArry[i]).addClass("haveimg_sltall-click").siblings().removeClass("haveimg_sltall-click"),AllaskRight()})})}}require("jquery");var Imgscroll=(require("js/modules/Magnifier.js"),require("js/modules/scrolling.js"));$(function(){$(".ringbuy_Details-tab span").each(function(index){$(this).live("click",function(){var type=$(this).data("type");1==type&&$.post(getcomments,{},function(res){getPinlun(res)}),$(this).addClass("Details-tab-click").siblings().removeClass("Details-tab-click"),$(".ringbuy_Details-all .Details-all").eq(index).show().siblings().hide(),AllaskRight()})}),$(".haveimg_bigimg-cort img").live("click",function(){$(this).parents(".haveimg_theimg").hide(),$(this).parents(".Details_allask-haveimg").find(".haveimg_sltall li").removeClass("haveimg_sltall-click"),$(".Details_allask-xq,.Details_allask-person").css("marginTop",0),AllaskRight()}),$(".nextPage, .prevPage").live("click",function(){var url=$(this).data("href");$.post(url,{},function(res){getPinlun(res)})})})});