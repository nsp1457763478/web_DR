ity=identity,identity=identity.replace("(","").replace(")","");var flag=!1;if(8==identity.length?(flag=HKIdValid(identity),0==flag&&(flag=MCIdValid(identity))):flag=TWIdValid(identity),0==flag&&10==_identity.length&&(flag=!0),0==flag)return $("#dr_yzwrong").html("護照編號輸入有誤，請重新輸入。"),$("#dr_yzwrong").show(),!1}else{if(1!=IdentityValid(identity)&&1==type)return $("#dr_yzwrong").html("身份证有误，请重新输入。"),$("#dr_yzwrong").show(),!1;if(2==type&&1!=PPIdValid(identity))return $("#dr_yzwrong").html("您的身份證號碼輸入有誤，請重新輸入。"),$("#dr_yzwrong").show(),!1}$("#formSubmit").submit()})})});