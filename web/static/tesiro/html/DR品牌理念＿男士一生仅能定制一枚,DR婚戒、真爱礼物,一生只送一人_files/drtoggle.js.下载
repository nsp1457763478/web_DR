/*!uglify*/
define(function(c,e,d){function b(){this.cfg={id:"",action:"",callback:null}}c("jquery"),b.prototype={init:function(b){var a=this;a.cfg=$.extend(a.cfg,b),$(a.cfg.id).off("mouseenter").on("mouseenter",function(){$(a.cfg.action).show(),$("#shopNav .drstore-xz").attr("style","width:26px"),$("#shopNav li").removeClass("drstore-tabhover").eq(0).addClass("drstore-tabhover"),$("#shopNav li.drstore-tabhover .drstore-shopname span").css("opacity","1")}).off("mouseleave").on("mouseleave",function(){$(a.cfg.action).hide(),$(".drstore-all .drstore-allline .drstore-xz").attr("style","width:139px"),$(".drstore-all .drstore-tab li").removeClass("drstore-tabhover").eq(0).addClass("drstore-tabhover"),$(".drstore-all .drstore-tabhover .drstore-shopname span").css("opacity","1")})}},d.exports=b});